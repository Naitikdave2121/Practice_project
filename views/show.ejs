<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>show data</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>
        <table class="table table-border text-center shadow">
            <tr>
                <th>id</th>
                <th>name</th>
                <th>email</th>
                <th>password</th>
                <th>phone</th>
                <!-- <th>no people</th> -->
               <th>image</th>
            </tr>
            <% data.forEach((d)=>{ %>          
                <tr>
                    <td align="center"><%=d._id %></td>
                    <td align="center"><%=d.name %></td>
                    <td align="center"><%=d.email %></td>
                    <td align="center"><%=d.password %></td>
                    <td align="center"><%=d.phone %></td>
                    <!-- <td align="center"><%=d.number_people %></td> -->
                    <td><img src="./image/<%=d.image %>" alt="not found" height="60px" width="90px"></td>
                </tr>
                <% }) %>
          
        </table>
</body>
</html>